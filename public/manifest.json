{
  "short_name": "tabs",
  "name": "tabs",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "tabs"
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Space",
        "mac": "MacCtrl+Space"
      }
    }
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "icons": {
    "16": "Assets/Icon_16.png",
    "32": "Assets/Icon_32.png",
    "48": "Assets/Icon_48.png",
    "128": "Assets/Icon_128.png"
  },
  "version": "1.2"
}
